{
  "name": "effect-ai-cli",
  "version": "0.1.0",
  "description": "Effect-based AI CLI with pluggable models and observability",
  "license": "MIT",
  "type": "module",
  "private": false,
  "bin": {
    "effect-ai": "./dist/src/bin/effect-ai-cli.js",
    "effect-ai-cli": "./dist/src/bin/effect-ai-cli.js"
  },
  "main": "./dist/src/main.js",
  "types": "./dist/src/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/main.js",
      "types": "./dist/src/main.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prepare": "bun run build"
  },
  "dependencies": {
    "effect": "^3.17.7",
    "@effect/platform": "^0.90.2",
    "@effect/platform-node": "^0.90.0",
    "@effect/ai-google": "^0.4.0",
    "@effect/ai-openai": "^0.28.2",
    "@effect/ai-anthropic": "^0.15.1",
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tsx": "^4.19.0",
    "rimraf": "^5.0.5"
  }
}
