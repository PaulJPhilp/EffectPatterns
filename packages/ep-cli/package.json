    "effect": "^3.19.14",
    "effect-cli-tui": "^2.2.0",
    "glob": "^11.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "liquidjs": "^10.24.0",
    "ora": "^9.0.0",
    "semver": "^7.7.3",
