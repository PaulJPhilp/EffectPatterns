{
  "name": "@effect-patterns/ep-shared-services",
  "version": "1.0.1",
  "description": "Shared services for Effect Patterns CLI tools",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tui": {
      "types": "./dist/tui/index.d.ts",
      "import": "./dist/tui/index.js"
    },
    "./logger": {
      "types": "./dist/logger/index.d.ts",
      "import": "./dist/logger/index.js"
    },
    "./display": {
      "types": "./dist/display/index.d.ts",
      "import": "./dist/display/index.js"
    },
    "./execution": {
      "types": "./dist/execution/index.d.ts",
      "import": "./dist/execution/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "cli",
    "services",
    "shared"
  ],
  "author": "Effect Patterns",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulJPhilp/Effect-Patterns.git",
    "directory": "packages/ep-shared-services"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/Effect-Patterns/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/Effect-Patterns#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "effect": "3.19.17",
    "@effect/platform": "0.94.4",
    "@effect/platform-node": "0.104.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "effect-cli-tui": "^2.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}