{
  "type": "module",
  "name": "effect-patterns-hub",
  "version": "0.1.0",
  "private": true,
  "description": "A community-driven knowledge base for Effect-TS patterns.",
  "scripts": {
    "publish": "bun run scripts/publish/publish.ts",
    "validate": "bun run scripts/publish/validate.ts",
    "generate": "bun run scripts/publish/generate.ts",
    "test": "bun run scripts/publish/test.ts",
    "rules": "bun run scripts/publish/rules.ts",
    "ingest": "bun run scripts/ingest/process.ts",
    "pipeline": "bun run scripts/publish/pipeline.ts",
    "all": "bun run pipeline",
    "lint:examples": "eslint \"content/src/**/*.ts\"",
    "typecheck:examples": "tsc --noEmit",
    "typecheck": "tsc --noEmit --strict",
    "test:llm-service": "vitest run cli/services/llm-service/__tests__/service.test.ts",
    "test:llm-service:watch": "vitest cli/services/llm-service/__tests__/service.test.ts",
    "test:llm-service:ui": "vitest --ui cli/services/llm-service/__tests__/service.test.ts",
    "test:llm-service:coverage": "vitest run cli/services/llm-service/__tests__/service.test.ts --coverage",
    "cli:build": "tsc --build cli",
    "cli:start": "bun run cli/src/main.ts",
    "cli:help": "bun run cli/src/main.ts --help",
    "cli:dev": "tsx watch cli/src/main.ts",
    "cli:lint": "biome check cli",
    "cli:lint:fix": "biome check --apply cli",
    "cli:format": "biome format --write cli",
    "cli:test": "vitest run cli",
    "cli:test:watch": "vitest cli",
    "cli:test:ui": "vitest --ui cli",
    "cli:test:coverage": "vitest run cli --coverage",
    "preqa:process": "tsc -b cli",
    "qa:process": "./scripts/qa/qa-process.sh",
    "qa:report": "bun run scripts/qa/qa-report.ts",
    "qa:repair": "bun run scripts/qa/qa-repair.ts",
    "qa:status": "bun run scripts/qa/qa-status.ts",
    "qa:repair:dry": "bun run scripts/qa/qa-repair.ts --dry-run",
    "qa:all": "./scripts/qa/qa-process.sh && bun run qa:report"
  },
  "dependencies": {
    "@effect/ai": "^0.25.0",
    "@effect/ai-anthropic": "^0.15.0",
    "@effect/ai-google": "^0.4.0",
    "@effect/ai-openai": "^0.28.0",
    "@effect/cli": "^0.69.0",
    "@effect/platform": "^0.90.0",
    "@effect/platform-node": "^0.94.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "dotenv": "^17.2.1",
    "effect": "^3.17.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "liquidjs": "^10.21.1",
    "process": "^0.11.10",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.31.2",
    "@types/bun": "latest",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "ts-patch": "^3.3.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "typescript",
    "patterns"
  ],
  "author": "Paul",
  "license": "MIT",
  "resolutions": {
    "@effect/platform": "^0.90.0",
    "@effect/platform-node": "^0.94.0",
    "effect": "^3.17.1"
  }
}